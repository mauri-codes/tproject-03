<% include _header %>
<h1>Bienvenid@</h1>
<% users.forEach(function(user) { %>
<div class="panel panel-default">
    <div class="<%= user.username%> panel-heading">
        <div class="panel-heading">
            <a href="/users/<%= user.username %>">
                <%= user.name() %>
            </a>
            <% if (currentUser) { %>
            <p class="delete btn btn-danger btn-xs pull-right" id="<%= user.username %>">Eliminar</p>
            <%}%>
        </div>
    </div>
    <% if (user.bio) { %>
    <div class="panel-body"><%= user.bio %></div>
    <% } %>
</div>
<% }) %>
<% include _footer %>